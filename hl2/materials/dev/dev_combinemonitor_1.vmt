"Unlittwotexture"
//"UnlitGeneric"

{
	"$basetexture" "_rt_Camera"
	"%tooltexture" "dev/dev_monitor"
	"$surfaceprop" "glass"
	"$additive" "1"

//	"$texture2"	"models/props_combine/shieldwall_tint"
	"$texture2"	"dev/dev_combinescanline"
//	"$texture2"	"engine/camerarendertarget"

	"$texoffset"	"[0.5 0.5]"
	"$texscale" "2"
	
	"Proxies"
	{

		"Sine"
		{
			"resultVar" "$color[0]"
			"sineperiod" .1
			"sinemin" .99
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" .1
			"sinemin" .99
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$color[2]"
			"sineperiod" .1
			"sinemin" .99
			"sinemax" 1
		}

		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
		
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" .06
			"texturescrollangle" -90
			"texturescale" 4
		}


	}




}
