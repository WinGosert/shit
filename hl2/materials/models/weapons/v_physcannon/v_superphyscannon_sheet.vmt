"VertexLitGeneric"
{
	"$baseTexture" "Models/Weapons/V_physcannon/v_superphyscannon_sheet"
	// "$envmap" "env_cubemap"
	// "$envmapmask" "Models/Weapons/V_physcannon/v_physcannon_mask"
	// "$envmapcontrast" ".7"
	// "$envmapsaturation" "[.8 .8 .8]"
	
	$detail Models/Weapons/V_physcannon/v_superphyscannon_detail
	$detailscale 1
	$detailblendfactor 1.0
	$detailblendmode 0
	
	"$bumpmap" "models/weapons/v_physcannon/v_superphyscannon_normal"
	
	"$phong" "1"
