"VertexLitGeneric"
{
	"$basetexture" "models\balloon\balloon_star"
	"$bumpmap" "models\balloon\balloon_star_normal"
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" "1"
	// "$color2" "[0.9 0.7 0.0]"
 
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "1"
	"$phongexponenttexture" "models\balloon\balloon_star"
	"$phongfresnelranges" "[0.1 0.5 1]"
 
	"$envmaptint" "[0.8 0.8 0.8]"
	
	"$rimlight" "1"
	"$rimlightboost" "3"
	"$rimlightexponent" "1"
	"$rimlightmask" "1"
	// "$rimlighttint" "[1 0.8 0.1]"
	
	Proxies
	{
		"Sine" // a proxy which produces a sine wave
		{
			resultVar $rimlightboost // The shader parameter to be manipulated
			sineperiod	3
			sinemin	 	1
			sinemax		 5
		}
	}
}
