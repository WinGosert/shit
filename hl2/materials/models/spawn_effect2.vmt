"UnlitGeneric"
{
 	$model 			"1"
	$basetexture 	"models/spawn_effect"
	$additive 		"1"
	$basetexturetransform "center .2 .6 scale 100 100 rotate 0 translate 0 0"
	$translucent 	"1"
	
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 1.0
			"texturescrollangle" 36
		}
	}
}
